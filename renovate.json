{
  "labels": ["maintenance"],
  "extends": ["config:base"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "rangeStrategy": "replace",
  "semanticCommitType": "fix",
  "packageRules": [
    {
      "depTypeList": ["devDependencies"],
      "semanticCommitType": "build"
    },
    {
      "depTypeList": ["action"],
      "semanticCommitType": "ci",
      "semanticCommitScope": "action"
    }
  ]
}
